From d8f72575b48b2fdbd84af926678b3ca9cc9cd910 Mon Sep 17 00:00:00 2001
From: hscham <hscham@chromium.org>
Date: Fri, 20 Nov 2020 15:25:48 +0900
Subject: [PATCH] Add new LoggingSeverity definitions

LOG_* will be renamed to LOGGING_*.

Change-Id: Ib6f301d137edf8ed32c13d17f2b719e694344cfc
---
 base/logging.h | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/base/logging.h b/base/logging.h
index 4bc8b00ef8..c9f0483511 100644
--- a/base/logging.h
+++ b/base/logging.h
@@ -352,6 +352,14 @@ const LogSeverity LOG_DFATAL = LOG_ERROR;
 const LogSeverity LOG_DFATAL = LOG_FATAL;
 #endif
 
+constexpr LogSeverity LOGGING_VERBOSE = LOG_VERBOSE;
+constexpr LogSeverity LOGGING_INFO = LOG_INFO;
+constexpr LogSeverity LOGGING_WARNING = LOG_WARNING;
+constexpr LogSeverity LOGGING_ERROR = LOG_ERROR;
+constexpr LogSeverity LOGGING_FATAL = LOG_FATAL;
+constexpr LogSeverity LOGGING_NUM_SEVERITIES = LOG_NUM_SEVERITIES;
+constexpr LogSeverity LOGGING_DFATAL = LOG_DFATAL;
+
 // A few definitions of macros that don't generate much code. These are used
 // by LOG() and LOG_IF, etc. Since these are used all over our code, it's
 // better to have compact code for these operations.
