base::ObserverList::might_have_observers().

Change-Id: I80ab6a72ce5803393203304627d4024dc12f7f54
---
 base/observer_list.h | 1 +
 1 file changed, 1 insertion(+)

diff --git a/base/observer_list.h b/base/observer_list.h
index 834ee64eee..3dd2353ff9 100644
--- a/base/observer_list.h
+++ b/base/observer_list.h
@@ -317,6 +317,7 @@ class ObserverList {
   }
 
   bool might_have_observers() const { return !observers_.empty(); }
